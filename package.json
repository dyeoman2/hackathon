{
  "name": "hackathon",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "author": "Daniel Yeoman",
  "version": "1.0.0",
  "description": "Hackathon management platform built with TanStack Start and Convex, featuring automated submission processing, AI-powered judging tools, live reveal ceremonies, and comprehensive competition workflows.",
  "homepage": "https://github.com/dyeoman2/hackathon",
  "repository": {
    "type": "git",
    "url": "https://github.com/dyeoman2/hackathon.git"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "tanstack start",
    "react",
    "typescript",
    "hackathon",
    "judging",
    "competition",
    "platform",
    "fullstack",
    "authentication",
    "convex",
    "resend",
    "netlify"
  ],
  "scripts": {
    "dev": "concurrently -n vite,convex -c blue,green \"NODE_OPTIONS='--import ./instrument.server.mjs' vite dev\" \"npx convex dev\"",
    "dev:full": "tsx scripts/dev-full.ts",
    "build": "vite build && mkdir -p .output/server && cp instrument.server.mjs .output/server",
    "start": "node --import ./.output/server/instrument.server.mjs ./.output/server/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "fix": "biome check --write .",
    "knip": "knip",
    "knip:prod": "knip --production",
    "setup:env": "npx tsx scripts/setup-env.ts",
    "setup:convex": "tsx scripts/setup-convex.ts",
    "setup:dev": "tsx scripts/setup-dev.ts",
    "setup:prod": "tsx scripts/setup-prod.ts",
    "infra:deploy": "npx alchemy deploy",
    "infra:create-r2-token": "tsx scripts/create-r2-token.ts",
    "infra:setup": "tsx scripts/setup-r2-infra.ts",
    "infra:setup:prod": "tsx scripts/setup-r2-infra.ts --prod",
    "precommit": "biome check --write",
    "updates": "npx npm-check-updates",
    "analyze": "pnpm build && npx vite-bundle-analyzer dist/client/assets/*.js",
    "test:webhook": "tsx scripts/test-webhook.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.31",
    "@ai-sdk/openai": "^2.0.65",
    "@aws-sdk/client-s3": "^3.931.0",
    "@aws-sdk/s3-request-presigner": "^3.931.0",
    "@convex-dev/better-auth": "^0.9.7",
    "@convex-dev/rate-limiter": "^0.3.0",
    "@convex-dev/resend": "^0.1.13",
    "@mendable/firecrawl-js": "^4.5.0",
    "@netlify/vite-plugin-tanstack-start": "^1.1.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/profiling-node": "^10.25.0",
    "@sentry/tanstackstart-react": "^10.25.0",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-router": "^1.136.1",
    "@tanstack/react-router-devtools": "^1.136.1",
    "@tanstack/react-start": "^1.136.1",
    "@tanstack/react-table": "^8.21.3",
    "@useautumn/convex": "^0.0.15",
    "adm-zip": "^0.5.16",
    "ai": "^5.0.93",
    "alchemy": "^0.77.5",
    "autumn-js": "^0.1.47",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.29.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.553.0",
    "next-themes": "^0.4.6",
    "npm-check-updates": "^19.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "resend": "^4.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "vite": "7.2.2",
    "workers-ai-provider": "^2.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "knip": "^5.69.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
